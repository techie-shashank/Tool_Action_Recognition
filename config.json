{
    "batch_size": 128,
    "epochs": 50,
    "learning_rate": 0.001,
    "data_ratio": 1.0,
    "semi_supervised": {
        "active": false,
        "strategy": "mean_teacher",
        "labelled_ratio": 0.25,
        "consistency_weight": 1.5
    },
    "data_balancing": [
        "oversample"
    ],
    "l1_lambda": 0.1,
    "gradient_clip": 0.5,
    "model_params": {
        "tcn": {
            "num_channels": [
                64,
                128,
                64
            ],
            "kernel_size": 5,
            "dropout": 0.4
        },
        "lstm": {
            "hidden_size": 64,
            "num_layers": 2,
            "dropout": 0.05
        }
    }
}