{
    "batch_size": 128,
    "epochs": 30,
    "learning_rate": 0.002,
    "data_ratio": 1.0,
    "semi_supervised": {
        "active": true,
        "strategy": "contrastive",
        "labelled_ratio": 0.25
    },
    "data_balancing": [
        "oversample",
        "augment"
    ],
    "l1_lambda": 0.1,
    "gradient_clip": 0.5,
    "model_params": {
        "tcn": {
            "num_channels": [
                64,
                128,
                64
            ],
            "kernel_size": 5,
            "dropout": 0.3
        },
        "lstm": {
            "hidden_size": 32,
            "num_layers": 1,
            "dropout": 0.1
        }
    }
}